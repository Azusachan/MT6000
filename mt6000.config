# 编译MT6000固件:
CONFIG_TARGET_mediatek=y
CONFIG_TARGET_mediatek_filogic=y
CONFIG_TARGET_mediatek_filogic_DEVICE_glinet_gl-mt6000=y
CONFIG_TARGET_OPTIMIZATION="-O2 -pipe -mcpu=cortex-a53"
CONFIG_TARGET_OPTIONS=y
CONFIG_TARGET_ROOTFS_PARTSIZE=104
CONFIG_PACKAGE_rtl8192cu-firmware=y

# 文件系统镜像等支持:
# CONFIG_TARGET_ROOTFS_SQUASHFS=y
# CONFIG_TARGET_ROOTFS_EXT4FS is not set
# CONFIG_TARGET_SQUASHFS_BLOCK_SIZE=256
# CONFIG_TARGET_UBIFS_FREE_SPACE_FIXUP=y
# CONFIG_TARGET_UBIFS_JOURNAL_SIZE=""

# IPv6支持:
CONFIG_PACKAGE_dnsmasq-full=y
CONFIG_PACKAGE_dnsmasq_full_auth=y
CONFIG_PACKAGE_dnsmasq_full_conntrack=y
CONFIG_PACKAGE_dnsmasq_full_dhcp=y
CONFIG_PACKAGE_dnsmasq_full_dhcpv6=y
CONFIG_PACKAGE_dnsmasq_full_dnssec=y
CONFIG_PACKAGE_dnsmasq_full_ipset=y
CONFIG_PACKAGE_dnsmasq_full_noid=y
CONFIG_PACKAGE_dnsmasq_full_tftp=y
CONFIG_PACKAGE_ip6tables=y
CONFIG_PACKAGE_ipv6helper=y
# CONFIG_PACKAGE_dnsmasq is not set
CONFIG_PACKAGE_luci-proto-ipv6=y

# tailscale
CONFIG_PACKAGE_tailscale=y
CONFIG_PACKAGE_iptables-nft=y

# DDNS
CONFIG_PACKAGE_ca-certificates=y
CONFIG_PACKAGE_ddns-scripts-cloudflare=y

#cloudflared
CONFIG_PACKAGE_cloudflared=y

# 常用LuCI插件选择:
CONFIG_PACKAGE_boost=y
CONFIG_PACKAGE_boost-date_time=y
CONFIG_PACKAGE_boost-program_options=y
CONFIG_PACKAGE_boost-system=y
CONFIG_boost-compile-visibility-hidden=y
CONFIG_boost-runtime-shared=y
CONFIG_boost-static-and-shared-libs=y
CONFIG_boost-variant-release=y
CONFIG_PACKAGE_ipt2socks=y
CONFIG_PACKAGE_kcptun-config=y
CONFIG_PACKAGE_kmod-lib-crc32c=y
CONFIG_PACKAGE_libatomic=y
CONFIG_PACKAGE_libevent2=y

# Mobile sharing
CONFIG_PACKAGE_kmod-usb-net-rndis=y
CONFIG_PACKAGE_kmod-usb-net-ipheth=y
CONFIG_PACKAGE_usbmuxd=y
CONFIG_PACKAGE_libimobiledevice=y
